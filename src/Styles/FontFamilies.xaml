<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Format to adding a new font: [location].ttf#[Font family name]-->
    <!-- Cascadia Mono (font included), with fallback options for non-compatible characters
         https://github.com/microsoft/cascadia-code/issues/55-->
    <FontFamily x:Key="CascadiaMonoFontFamily">ms-appx:///Assets/Fonts/CascadiaMono.ttf#Cascadia Mono,MS Gothic, NSimSun</FontFamily>
    <FontFamily x:Key="DevHomeFluentIcons">/Assets/DevHomeFluentIcons.ttf#DevHome Fluent Icons</FontFamily>
</ResourceDictionary>
